#include <iostream>
using namespace std;
int main()
{
    int s;
    int a[3][2];
    cin>>s;
    for (int i=0; i<3; i++)
    {
        for (int j=0; j<2; j++)
        {
            cin>>a[i][j];
        }
    }
    for (int i=0; i<3; i++)
    {
        for (int j=0; j<2; j++)
        {
            a[i][j]*=s;
        }
    }
    for (int i=0; i<3; i++)
    {
        for (int j=0; j<2; j++)
        {
            cout<<a[i][j]<<" ";
        }
    }
    return 0;
}
