#include <stdio.h>
int main()
{
    int flag=1, i, j, t, k, n;
    scanf("%d", &n);
    int a[n];
    for (i=0; i<n; i++)
    {
        scanf("%d", &a[i]);
    }
    for(j=0; j<n-1 && flag==1; j++)
    {
        flag=0;
        for(i=0; i<n-j-1; i++)
        {
            if (a[i]>a[i+1])
            {
                t=a[i];
                a[i]=a[i+1];
                a[i+1]=t;
                flag=1;
            }
        }
    }
    for (i=0; i<n; i++)
    {
        printf("%d", a[i]);
    }
    return 0;
}
