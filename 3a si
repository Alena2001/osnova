#include <stdio.h>

int main()
{
   int s;
    int a[3][2];
    scanf("%d", &s);
    for (int i=0; i<3; i++)
    {
        for (int j=0; j<2; j++)
        {
            scanf("%d", &a[i][j]);
        }
    }
    for (int i=0; i<3; i++)
    {
        for (int j=0; j<2; j++)
        {
            a[i][j]*=s;
        }
    }
    for (int i=0; i<3; i++)
    {
        for (int j=0; j<2; j++)
        {
            printf("%d\t", a[i][j]);
        }
    }
    return 0;
}
